<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
    <display-name>render .png image files for SMILES strings, NSC numbers</display-name>
    <servlet>
        <servlet-name>StructureServlet</servlet-name>
        <servlet-class>mwk.structureservlet.structureservlet.StructureServletCDK</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>StructureServlet</servlet-name>
        <url-pattern>/StructureServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
